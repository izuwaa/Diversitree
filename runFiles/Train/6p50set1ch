#PBS -o /lustre/haven/user/iahanor/branchandbound/Train/6p/6p50set1chO.txt
#PBS -e /lustre/haven/user/iahanor/branchandbound/Train/6p/6p50set1chE.txt
#PBS -l walltime=24:00:00
